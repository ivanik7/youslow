{
    "manifest_version": 3,
    "name": "youSlow",
    "version": "0.2.0",
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*"
            ],
            "js": [
                "youslow-inject.js"
            ]
        }
    ],
    "description": "Granular video speed control on YouTube",
    "browser_specific_settings": {
        "gecko": {
            "id": "youslow@ivanik.ru"
        }
    },
    "icons": {
        "48": "icons/icon48.png",
        "96": "icons/icon96.png"
    }
}